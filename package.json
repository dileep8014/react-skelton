  "description": "boiler plate app",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "test": "jest",
    "build": "webpack --config webpack.config.prod.js --progress --display-error-details"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dileep8014/react-skelton.git"
